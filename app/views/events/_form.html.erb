<%= form_for @event do |f|%>

	<%= f.label :title %>
	<%= f.text_field :title, class: 'form-control'%>

	<%= f.label :note %>
	<%= f.text_area :note, class: 'form-control'%>

	<%= f.label :date%>
	<%= f.text_field :date, class: 'form-control'%>

	<%= f.label :time%>
	<%= f.text_field :time, class: 'form-control'%>

	
	<%= f.submit%>
	

<%end%>

